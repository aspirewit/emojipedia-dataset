{
    "_id": "emojipedia",
    "startUrl": [
        "https://emojipedia.org/"
    ],
    "selectors": [
        {
            "id": "category",
            "type": "SelectorLink",
            "parentSelectors": [
                "_root"
            ],
            "selector": "div.block:nth-of-type(1) li a",
            "multiple": true,
            "delay": 0
        },
        {
            "id": "category-name",
            "type": "SelectorText",
            "parentSelectors": [
                "category"
            ],
            "selector": ".content h1",
            "multiple": false,
            "regex": ".{3,}",
            "delay": 0
        },
        {
            "id": "category-description",
            "type": "SelectorText",
            "parentSelectors": [
                "category"
            ],
            "selector": ".container .content p",
            "multiple": false,
            "regex": "",
            "delay": 0
        },
        {
            "id": "category-emoji-item",
            "type": "SelectorLink",
            "parentSelectors": [
                "category"
            ],
            "selector": ".emoji-list li a",
            "multiple": true,
            "delay": 0
        },
        {
            "id": "emoji",
            "type": "SelectorText",
            "parentSelectors": [
                "category-emoji-item"
            ],
            "selector": "h1 span",
            "multiple": false,
            "regex": "",
            "delay": 0
        },
        {
            "id": "emoji-name",
            "type": "SelectorHTML",
            "parentSelectors": [
                "category-emoji-item"
            ],
            "selector": "article h1",
            "multiple": false,
            "regex": "(?<=<span class=\"emoji\">.+</span>).+",
            "delay": 0
        },
        {
            "id": "emoji-description",
            "type": "SelectorText",
            "parentSelectors": [
                "category-emoji-item"
            ],
            "selector": "section.description",
            "multiple": false,
            "regex": "(.+\\n{1,})+(?=Copy and paste this emoji:)",
            "delay": 0
        }
    ]
}
